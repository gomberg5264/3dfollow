{% extends 'default_page.html.twig' %}

{% block title %}{{ 'account.export_data.meta_title'|trans }}{% endblock %}
{% block page_title %}{{ 'account.export_data.page_title'|trans }}{% endblock %}

{% block page_body %}
    <p>
        {{ 'account.export_data.content'|trans|nl2br }}
    </p>
    {{ form_start(form, {attr: {class: 'mt-5 mb-3'}}) }}
    {{ form_end(form) }}

    {% embed 'common/form_actions.html.twig' with {button_label: 'account.export_data.button_submit'|trans, button_icon: 'file-export', back_to_action_link: path('dashboard_index'), back_to_action_label: 'common.action.back_to_dashboard'|trans} %}
    {% endembed %}
{% endblock %}
